{
  "data_source": "actions",
  "batch_size": 10,
  "max_retries": 3,
  "primary_key": "composite_key",
  "last_modified_column": "last_modified_on",
  "main_table": "dl_issues_actions",
  "child_tables": [
    "dl_issues_actions_hierarchy"
  ],
  "parquet_mapping": {
    "dl_issues_actions": "issues_actions.parquet",
    "dl_issues_actions_hierarchy": "issues_actions_hierarchy.parquet"
  },
  "stages": [
    {
      "name": "insert_main",
      "type": "ingestion",
      "target": "main"
    },
    {
      "name": "insert_children",
      "type": "ingestion",
      "target": "children",
      "depends_on": ["insert_main"]
    }
  ]
}
